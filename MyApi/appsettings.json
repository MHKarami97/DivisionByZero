{
  "ConnectionStrings": {
    "SqlServer": "Data Source=.;Initial Catalog=DivisionByZeroDb;Integrated Security=true",
    "Elmah": "Data Source=.;Initial Catalog=DivisionByZeroLogDb;Integrated Security=true"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SiteSettings": {
    "ElmahPath": "/elmah-errors",
    "JwtSettings": {
      "SecretKey": "LongerThan-16Char-SecretKey",
      "Encryptkey": "16CharEncryptKey",
      "Issuer": "MyAPIKeysIssuer",
      "Audience": "MyAPIKeysAudience",
      "NotBeforeMinutes": "0",
      "ExpirationMinutes": "43800",
      "RefreshTokenExpirationMinutes": "500000",
      "AllowMultipleLoginsFromTheSameUser": "true"
    },
    "IdentitySettings": {
      "PasswordRequireDigit": "true",
      "PasswordRequiredLength": "6",
      "PasswordRequireNonAlphanumic": "false",
      "PasswordRequireUppercase": "false",
      "PasswordRequireLowercase": "true",
      "RequireUniqueEmail": "true"
    }
  }
}
